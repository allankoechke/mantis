PROJECT_NAME           = Mantis App
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/docs
INPUT                  = @PROJECT_SOURCE_DIR@/include @PROJECT_SOURCE_DIR@/src @CMAKE_SOURCE_DIR@/doc @CMAKE_SOURCE_DIR@/README.md
RECURSIVE              = YES
EXTRACT_ALL            = YES
GENERATE_LATEX         = NO
GENERATE_HTML          = YES
HAVE_DOT               = YES
DOT_GRAPH_MAX_NODES    = 100
CALL_GRAPH             = NO
CALLER_GRAPH           = NO
QUIET                  = YES
MARKDOWN_SUPPORT       = YES
PROJECT_NUMBER         = v@PROJECT_VERSION@
HTML_EXTRA_STYLESHEET  = @AWESOME_CSS_DIR@/doxygen-awesome.css
USE_MDFILE_AS_MAINPAGE = @DOXY_MAINPAGE@
FILE_PATTERNS          = *.h *.cpp *.md
PROJECT_LOGO           = @DOXYGEN_LOGO@